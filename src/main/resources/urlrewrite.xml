<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>
    <rule>
        <from>^/a/actualites-(.*)$</from>
        <to>/test?id=$1</to>
    </rule>
    <rule>
        <from>^/category/([a-z]+)-([0-9]+)\.html$</from>
        <to>/categorie?idgenre=$2</to>
    </rule>
</urlrewrite>